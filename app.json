{
  "id": "com.e-domotica",
  "sdk": 1,
  "name": {
    "en": "e-Domotica"
  },
  "description": {
    "en": "Adds support for e-Domotica Z-Wave devices."
  },
  "category": "lights",
  "version": "1.0.2",
  "compatibility": ">=1.5.0",
  "author": {
    "name": "Athom B.V.",
    "email": "apps@athom.com"
  },
  "contributors": {
    "developers": [
      {
        "name": "Ralf van Dooren",
        "email": "ralfvd@gmail.com"
      }
    ]
  },
  "images": {
    "large": "/assets/images/large.png",
    "small": "/assets/images/small.png"
  },
  "drivers": [
    {
      "id": "EM6550",
      "name": {
        "en": "Wall Plug (EM6550)"
      },
      "class": "socket",
      "capabilities": [
        "onoff"
      ],
      "zwave": {
        "manufacturerId": 96,
        "productTypeId": [
